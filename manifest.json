{
  "manifest_version": 2,

  "name": "LISTS",
  "description": "A chrome extension that helps you find sites about the article you are reading.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs",
    "http://*/",
    "https://*/"
  ]
}

